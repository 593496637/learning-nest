# 开发环境配置
NODE_ENV=development
PORT=3000
API_PREFIX=api
LOG_LEVEL=debug

# 开发数据库
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=nest_learning_dev

# 开发环境可以使用简单的 JWT 密钥
JWT_SECRET=dev-secret-key
